---
import NavBarSelection from "./HeroSection/NavBarSelection";
---

<div
  class="flex px-10 py-5 z-10 bg-bgColor text-primary justify-between items-center"
>
  <h1 id="logo" class="text-4xl">Logo</h1>

  <NavBarSelection client:load />
</div>

<script>
  const h1 = document.getElementById("logo");

  setTimeout(() => {
    h1?.classList.add("text-4xl");
  }, 4000);
</script>
